        </div>

        <!-- ノート個別フィードバック用モーダル（ノート確認アプリ統合版） -->
        <div id="feedbackModalBackdrop" class="modal-backdrop" style="display: none;">
          <div class="modal modal-notebook">
            <div class="modal-header">
              <div>
                <span id="modalStudentLabel">生徒：</span>
              </div>
              <div class="modal-header-controls">
                <label>
                  ペン色：
                  <input id="penColorInput" class="small-input" type="color" value="#ff0000" />
                </label>
                <label>
                  ペン太さ：
                  <input id="penWidthInput" class="small-input" type="number" value="3" min="1" max="20" />px
                </label>
                <button id="eraserToggleBtn" class="share-off">消しゴムOFF</button>
                <button id="clearAnnotationBtn" class="share-off">
                  手書きクリア
                </button>
                <button id="shareToggleBtn" class="share-off">共有開始</button>
                <button id="feedbackModalCloseBtn" class="close-btn">閉じる</button>
              </div>
            </div>
            <div class="modal-body">
              <canvas id="feedbackCanvas"></canvas>
            </div>
          </div>
        </div>

        <!-- 生徒画面の拡大表示用モーダル（高機能版：パン・ズーム・書き込み対応） -->
        <div id="modalBackdrop" class="modal-backdrop">
          <div class="modal modal-notebook"
            style="width: 90%; height: 90%; max-width: none; display: flex; flex-direction: column;">
            <!-- ★ キャンバスを全画面表示 -->
            <div class="modal-body" style="position: relative; overflow: hidden; background: #f1f5f9; flex: 1;">
              <div id="modalBoardContainer" style="width: 100%; height: 100%;">
                <canvas id="modalCanvas"></canvas>
              </div>

              <!-- ★ ツールバーをキャンバスの上にオーバーレイ -->
              <div
                style="position: absolute; top: 0; left: 0; right: 0; background: rgba(255, 255, 255, 0.95); backdrop-filter: blur(8px); padding: 12px 16px; display: flex; justify-content: space-between; align-items: center; box-shadow: 0 2px 8px rgba(0,0,0,0.1); z-index: 10;">
                <div style="display: flex; align-items: center; gap: 16px;">
                  <span id="modalTitle" class="modal-title" style="font-weight: 600; font-size: 16px;">生徒画面</span>
                  <!-- ツールバー -->
                  <div class="tool-row" style="display: inline-flex; gap: 8px;">
                    <button id="modalToolSelect" class="tool-button active" title="選択/パン">
                      <span class="icon">🖱️</span>
                    </button>
                    <button id="modalToolPen" class="tool-button" title="ペン">
                      <span class="icon">✏️</span>
                    </button>
                    <button id="modalToolEraser" class="tool-button" title="消しゴム">
                      <span class="icon">🧽</span>
                    </button>
                    <label style="display: inline-flex; align-items: center; gap: 4px;">
                      色:
                      <input id="modalPenColor" type="color" value="#ff0000" />
                    </label>
                  </div>
                </div>
                <button id="modalCloseBtn" class="close-btn">閉じる</button>
              </div>
            </div>
          </div>
        </div>

        <!-- socket.io -->
        <script src="/socket.io/socket.io.js"></script>

        <!-- モジュール -->
        <script type="module" src="/js/teacher.js"></script>

        <!-- チャットパネル -->
        <button id="chatToggleBtn" class="chat-toggle-btn" title="チャットを開く">
          💬
          <span id="chatNotifyDot" class="chat-notify-dot"></span>
        </button>

        <aside id="chatPanel" class="chat-panel collapsed">
          <header class="chat-panel-header">
            <h3 class="chat-title">クラスチャット</h3>
            <div id="chatTargetArea" class="chat-target-area">
              <label>
                宛先：
                <select id="chatTargetSelect">
                  <option value="">生徒を選択</option>
                </select>
              </label>
            </div>
            <button id="chatCloseBtn" class="chat-close-btn">×</button>
          </header>

          <div id="chatMessages" class="chat-messages"></div>

          <footer class="chat-input-area">
            <input id="chatInput" type="text" placeholder="メッセージを入力" autocomplete="off" />
            <button id="chatSendBtn">送信</button>
          </footer>
        </aside>

        <!-- スタンプ候補パレット -->
        <div id="stampPalette" class="stamp-palette stamp-palette-hidden">
          <div class="stamp-palette-inner">
            <div class="stamp-palette-header">
              <span>スタンプを選択</span>
              <button id="stampPaletteCloseBtn" class="stamp-palette-close">×</button>
            </div>
            <div class="stamp-palette-grid">
              <!-- board-ui.js が wb.stampPresets から作成 -->
            </div>
          </div>
        </div>

        <!-- 図形パレット -->
        <div id="shapePalette" class="stamp-palette stamp-palette-hidden shape-palette">
          <div class="stamp-palette-inner shape-palette-inner">
            <div class="stamp-palette-header">
              <span>図形を選択</span>
              <button id="shapePaletteCloseBtn" class="stamp-palette-close">×</button>
            </div>
            <div class="shape-items"></div>
          </div>
        </div>
</body>

</html>